# Slack Bot Configuration
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_BOT_TOKEN_CONTENTS=xoxb-your-contents-bot-token
SLACK_BOT_TOKEN_DATA=xoxb-your-data-bot-token
SLACK_APP_TOKEN=xapp-your-app-token
SLACK_APP_TOKEN_CONTENTS=xapp-your-contents-app-token
SLACK_APP_TOKEN_DATA=xapp-your-data-app-token

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key

# Anthropic Configuration (OOM Analysis)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key

# Notion Configuration
NOTION_TOKEN=secret_your-notion-integration-token

# Tavily Search Configuration
TAVILY_API_KEY=tvly-your-tavily-api-key

# AWS Athena Configuration (Phase 1 - Data Analysis Bot)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=ap-northeast-2
# Note: ATHENA_DATABASE 환경 변수 없음
# AI가 Redash 쿼리를 분석하여 항상 명시적으로 데이터베이스를 지정합니다
ATHENA_OUTPUT_LOCATION=s3://your-bucket/athena-results/

# Redash Configuration (Phase 1 - Data Analysis Bot)
REDASH_API_KEY=your-redash-api-key
REDASH_BASE_URL=https://your-redash-instance.com

# GitHub Configuration
# GITHUB_TOKEN: PR/리뷰 조회 등 읽기 작업용 (repo:read, read:org)
GITHUB_TOKEN=ghp_your-github-personal-access-token

# GITHUB_ADMIN_TOKEN: Organization 관리용 - scripts/github 전용 (repo, admin:org, write:org)
GITHUB_ADMIN_TOKEN=ghp_your-admin-personal-access-token
GITHUB_ORG_NAME=your-organization-name
